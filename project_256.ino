void setup() {
  Serial.begin(9600);

  ]
  if (!oled.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("failed to start SSD1306 OLED"));
    while (1);
  }

  delay(2000); 
  oled.setCursor(0, 0);
}

void loop() {
  oled.clearDisplay();

 
  oled.drawBitmap(0, 0, arduino_icon, 128, 64, WHITE);
  oled.display();
  delay(1000);


  oled.invertDisplay(1);
  delay(1000);
}
